    {
        "id": "41bafb6a9265fe18",
        "type": "function",
        "z": "d3bafc4d888d1327",
        "name": "Escalizacion de la Frecuencia",
        "func": "function escalarARPM(valor, rpmMin = 800, rpmMax = 1200) {\n    // Aplicar la fórmula para escalar el valor\n    var rpm = rpmMin + valor * (rpmMax - rpmMin);\n    // Redondear el valor de RPM al número entero más cercano\n    return Math.round(rpm);\n}\nvar entradaPID = msg.payload;\nmsg.payload = escalarARPM(entradaPID,800,1200);\nnode.status({fill:\"blue\",shape:\"dot\",text:\"Entrada :\" + entradaPID + ' Salida : ' + msg.payload});\nreturn msg;",
        "outputs": 1,
